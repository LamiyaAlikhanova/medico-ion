@{
}
 @model Doctor
<form method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="Name" class="col-form-label">Name</label>
        <input asp-for="Name" class="form-control">
        <span asp-validation-for="Name"></span>

    </div>
    <div class="form-group">
        <label asp-for="Position" class="col-form-label">Position</label>
        <input asp-for="Position" class="form-control">
        <span asp-validation-for="Position"></span>

    </div>
    <div class="form-group">
        <label asp-for="ImgFile" class="col-form-label">ImgFile</label>
        <input asp-for="ImgFile" class="form-control">
        <span asp-validation-for="ImgFile"></span>

    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>